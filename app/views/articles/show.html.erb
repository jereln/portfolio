<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @article.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @article.body %>
</p>

<p id="author">
  <b>By:</b>
  <%= @article.author.username %>
</p>

<p id="status">
	<b>Status:</b>
	<%= @article.published? ? "Published" : "Unpublished" %>
</p>

<%= link_to 'Edit', edit_article_path(@article) %> |
<%= link_to 'Back', articles_path %>
